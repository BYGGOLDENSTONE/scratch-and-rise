config_version=5

[application]

config/name="Scratch & Rise"
config/description="Kazı kazan + kripto/kumar meme kültürü temalı incremental oyun"
run/main_scene="res://scenes/main/Main.tscn"
config/features=PackedStringArray("4.6", "GL Compatibility")
